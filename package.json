{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "the essence of a react component",
  "main": "index.js",
  "scripts": {
    "start": "run-p client server",
    "dev": "run-p client:watch server",
    "client": "webpack --config ./client/webpack.config.js",
    "client:watch": "webpack  --config ./client/webpack.config.js --watch",
    "server": "node server",
    "lint": "eslint ."
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "keywords": [],
  "author": "EDA",
  "license": "ISC",
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.14",
    "babel-loader": "^8.2.2",
    "dotenv": "^16.0.0",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "antd": "^4.19.2",
    "@ant-design/icons": "^4.7.0",
    "axios": "^0.26.1",
    "chart.js": "^3.7.1",
    "html-react-parser": "^1.4.9",
    "millify": "^4.0.0",
    "moment": "^2.29.1",
    "superagent": "^5.3.1",
    "webpack": "^5.34.0",
    "webpack-cli": "^4.6.0"
